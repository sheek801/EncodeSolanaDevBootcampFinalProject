{
    "name": "solana-express-app",
    "version": "1.0.0",
    "description": "Express app for Solana price and transaction handling",
    "main": "dist/app.js",
    "type": "module",
    "scripts": {
        "start": "node --experimental-specifier-resolution=node dist/app.js",
        "build": "tsc",
        "dev": "node --loader ts-node/esm src/app.ts"
    },
    "dependencies": {
        "@solana/actions": "^1.6.4",
        "@solana/web3.js": "^1.73.0",
        "cors": "^2.8.5",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.12",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    }
}